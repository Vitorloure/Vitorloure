exercicio 3:
s = 0  
for num in range(1, 501):  
    if num % 2 != 0:  
        if num % 3 == 0:  
            s += num  
print("Resultado da soma de todos os múltiplos é: {}.".format(s))


exercicio 4:
n = int(input("Digite um número: "))
if n < 0:
    print("Digite apenas valores positivos, seu burro.")
if n == 0 or n == 1:
    print(f"{n} é um caso especial.")
else:
    if n == 2:
        print("2 é primo")
    elif n % 2 == 0:
        print(f"{n} não é primo.")
    else:
        x = 3
        while(x < n):
            if n % x == 0:
                break
            x = x + 2
        if x == n:
            print(f"{n} é primo")
        else:
            print(f"{n} não é primo, pois é divisível por {x}")
            
            
     from datetime import date

ano_atual = date.today().year 
ano = 0
maiores_idade = 0   
menores_idade = 0   

for c in range(0,6): 
    ano = int(input('Digite o seu ano de nascimento:'))
    idade_pessoa = ano_atual - ano  

    if idade_pessoa > 18:
        maiores_idade = maiores_idade + 1  
    else:
        menores_idade = menores_idade + 1 

print('Maiores de idade {}'.format(maiores_idade))
print('Menores de idade {}'.format(menores_idade))
