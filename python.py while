import time 
contador = 0
while contador < 10:
    print("nao deu,piá!")
    contador = contador + 1
    if contador == 6:
        break
    time.sleep(1)
print("Agora deu, bixo véio!")





num = int(input("Me informa um numero,meu bom: "))
fat = num
contador = 1
while (num-contador)>1:
    fat = fat*(num-contador)
    contador += 1
print(f"O fatorial de {num} é = {fat}")  




sexo = str(input("Me informe o seu sexo: (M para masculino e F para feminino):  ")).upper()

while sexo not in "MF":
   sexo = str(input("Informe um sexo valido: ")).upper()
print("Sexo valido!")
