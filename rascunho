    n1 = int(input("Número 1: "))
n2 = int(input("Número 2: "))
n3 = int(input("Número 3: "))


if n1 == n2 and n1 > n3:
    print("Os dois primeiros números são iguais!")

if n1 == n3 and n2 > n1:
    print("Primeiro e terceiro números são iguais!") 
    
if n1 == n2 and n3 > n1:
    print("Os dois primeiros números são iguais! ")
    
if n1 == n2 and n1 == n3:
    print("Numeros iguais")
    
elif n1 > n2 and n1 > n3:
    print("O primeiro número é maior ")

elif n2 > n1 and n2 > n3:
    print("O segundo número é maior ")
    
elif n3 > n2 and n3 > n1:
    print("O terceiro número é maior ")
    
if n1 < n2 and n1 < n3:
    print("O primeiro número é menor ")

if n2 < n1 and n2 < n3:
    print("O segundo número é menor ")
    
if n3 < n2 and n3 < n1:
    print("O terceiro número é menor ")
    
    
    
    
    
    
    
    
    
    
    
