nome = (input(f"Informe o seu nome: "))

if nome == "1":
  nome = input("Digite o nome novamente: ")
  
elif nome == "2":
  nome = input("Digite o nome novamente: ")
  
if nome == 'vitor':
  print("Nome bonito!")
else:
  print("Nome feio!")

