 idade = int(input("Me informe sua idade, bixo feio: "))
 
 
 
 
v = input('Digite um caractere: ')

if v == "a" or v == "A" or v == "e" or v == "E" or v == "i" or v == "I" or v == "o" or v == "O" or v == "u" or v == "U":
    print(f"A letra {v} é uma vogal")
else:
    print(f"A letra {v} é uma consoante")




nota1 = float(input("Nota 1: "))
nota2 = float(input("Nota 2: "))

med = (nota1 + nota2) / 2

print(f"Sua média final é: {med} ")

if med == 10:
    print("Aprovado com distição")
    
elif med < 7:
    print("Reprovado")
    
elif med >=7:
    print("Aprovado")
    
    
    
    
    
    
    
    
    
    
    
    
    
