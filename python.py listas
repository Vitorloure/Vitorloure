#remover valor a lista
minha_lista = [1, 2, 3, 4]
minha_lista.remove(1)
print(minha_lista)



#indexição da lista

number = ["1","2","3","4"]

number[0] = "1"
number[1] = "2"
number[2] = "3"
number[3] = "4"


#divisão de listas
print(number[0:3:1])




